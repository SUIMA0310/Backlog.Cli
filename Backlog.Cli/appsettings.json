{
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "logFile": {
        "type": "file",
        "fileName": "log.txt"
      },
      "console": {
        "type": "console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "logFile"
      },
      {
        "logger": "*",
        "minLevel": "Error",
        "writeTo": "console"
      }
    ]
  },
  "Backlog": {
    "BaseUrl": "https://example.backlog.jp/",
    "ApiKey": "<< Backlog Api Key >>",
    "ReadRateLimitCount": 8,
    "ReadRateLimitWindow": 1
  }
}